<androidx.cardview.widget.CardView android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:elevation="4dp"
    app:cardCornerRadius="7dp"
    android:layout_marginBottom="5dp">

        <LinearLayout
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:padding="16dp" android:orientation="vertical">


        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">

        <TextView android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:textColor="@color/colorTextHint"
            android:text="@string/place_name" android:layout_weight="2"/>

        <TextView android:layout_height="wrap_content"
            android:layout_width="match_parent" android:textColor="@color/darker_gray"
            android:text="@string/place_name" android:layout_weight="1" android:id="@+id/name"/>

    </LinearLayout>


        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="5dp">

        <TextView android:layout_height="wrap_content"
            android:layout_width="match_parent" android:textColor="@color/colorTextHint"
            android:text="@string/dist" android:layout_weight="2"/>

        <TextView android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:textColor="@color/darker_gray" android:text="@string/dist"
            android:layout_weight="1" android:id="@+id/distance"/>

    </LinearLayout>


        <LinearLayout android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="5dp">

        <TextView android:layout_height="wrap_content"
            android:layout_width="match_parent" android:textColor="@color/colorTextHint"
            android:text="@string/dur" android:layout_weight="2"/>

        <TextView android:layout_height="wrap_content" android:layout_width="match_parent"
            android:textColor="@color/darker_gray" android:text="@string/dur"
            android:layout_weight="1" android:id="@+id/duration"/>

    </LinearLayout>

            <LinearLayout android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="5dp">

                <TextView android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:textColor="@color/colorTextHint"
                    android:text="@string/slot" android:layout_weight="2"/>

                <TextView android:layout_height="wrap_content" android:layout_width="match_parent"
                    android:textColor="@color/darker_gray" android:text="@string/dur"
                    android:layout_weight="1" android:id="@+id/slot"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="end"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/direction"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded"
                    android:gravity="center"
                    android:paddingStart="22.3dp"
                    android:paddingTop="7.3dp"
                    android:paddingEnd="19dp"
                    android:paddingBottom="8.7dp"
                    android:text="Get Direction"
                    android:textColor="#ffffff"
                    android:textSize="15sp" />

            </LinearLayout>
    </LinearLayout>

    </androidx.cardview.widget.CardView>

